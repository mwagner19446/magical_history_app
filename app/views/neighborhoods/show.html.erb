<h1 id="tour-header"><%=@neighborhood.name%>: Sights to See</h1> <br \>


<div id="tour-container">

  <div id="map-canvas">
  </div>

  <div id="places">
    <% @places.each do |place| %>
      <input type=checkbox>
      <%=place.name%> <br \>
    <% end %>
  </div>

</div>

<div id="tour-latitude"><%=@neighborhood.latitude%></div>  
<div id="tour-longitude"><%=@neighborhood.longitude%></div>
  